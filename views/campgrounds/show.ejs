<% layout('layouts/boilerplate')%>
    <h1><%= camp.title  %>
    </h1>
    <h2><%= camp.location %>    </h2>
    <img src="<%= camp.image%>" alt="">
    <p><%= camp.description%></p>
    <footer>
       <p><a href="/campgrounds/<%=camp._id%>/edit">Edit</a></p>
       <p><form action="/campgrounds/<%=camp._id%>?_method=DELETE" method="POST">
        <button class="btn btn-danger">Delete</button>
       </form></p>
        <a href="/campgrounds">All Campgrounds</a>
    </footer>
